#include <stdio.h>
#include <string.h>

int main() {
    char a[100];  // 假設輸入字串長度不超過 100
    if (scanf("%99s", a) != 1) {
        return 0;
    }

    size_t len = strlen(a);
    for (size_t i = 0; i < len / 2; ++i) {
        char tmp = a[i];
        a[i] = a[len - 1 - i];
        a[len - 1 - i] = tmp;
    }

    printf("%s\n", a);
    return 0;
}
