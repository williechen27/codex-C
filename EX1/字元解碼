#include <ctype.h>
#include <stdio.h>

int main() {
    char a[100];  //假設輸入字串長度不超過 100
    if (scanf("%99s", a) != 1) {
        return 0;
    }

    for (int i = 0; a[i] != '\0';) {
        char current = a[i++];
        int count = 0;

        while (a[i] != '\0' && isdigit((unsigned char)a[i])) {
            count = count * 10 + (a[i] - '0');
            ++i;
        }

        if (count == 0) {
            putchar(current);
        } else {
            for (int j = 0; j < count; ++j) {
                putchar(current);
            }
        }
    }

    putchar('\n');
    return 0;
}

/*Enhanced Version*/

#include <ctype.h>
#include <stdbool.h>
#include <stdio.h>

int main(void) {
    char a[100];
    if (scanf("%99s", a) != 1) return 0;

    for (size_t i = 0; a[i] != '\0'; ) {
        unsigned char current = (unsigned char)a[i++];

        long long count = 0;      // 放大一點，避免過早溢位
        bool has_digits = false;  // 區分「沒有數字」與「數字=0」

        while (a[i] != '\0' && isdigit((unsigned char)a[i])) {
            has_digits = true;
            count = count * 10 + (a[i] - '0');
            ++i;
            // 可在此處加上上限檢查，例如 if (count > 1000000) break;
        }

        if (!has_digits) {
            putchar((int)current);
        } else {
            for (long long j = 0; j < count; ++j) {
                putchar((int)current);
            }
        }
    }

    putchar('\n');
    return 0;
}
