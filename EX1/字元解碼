#include <ctype.h>
#include <stdio.h>

int main() {
    char a[100];  //假設輸入字串長度不超過 100
    if (scanf("%99s", a) != 1) {
        return 0;
    }

    for (int i = 0; a[i] != '\0';) {
        char current = a[i++];
        int count = 0;

        while (a[i] != '\0' && isdigit((unsigned char)a[i])) {
            count = count * 10 + (a[i] - '0');
            ++i;
        }

        if (count == 0) {
            putchar(current);
        } else {
            for (int j = 0; j < count; ++j) {
                putchar(current);
            }
        }
    }

    putchar('\n');
    return 0;
}
